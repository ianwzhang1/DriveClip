<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="207.0" prefWidth="335.0" styleClass="background" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.ianwzhang1.driveclip.CAController">
   <children>
      <Button layoutX="105.0" layoutY="143.0" mnemonicParsing="false" onAction="#upload" text="Upload" />
      <Button layoutX="177.0" layoutY="143.0" mnemonicParsing="false" onAction="#download" text="Download" />
      <Button layoutX="115.0" layoutY="40.0" mnemonicParsing="false" onAction="#login" text="Login" />
      <Button layoutX="177.0" layoutY="40.0" mnemonicParsing="false" onAction="#logout" text="Logout" />
      <HBox alignment="CENTER" layoutX="38.0" layoutY="90.0" prefHeight="26.0" prefWidth="259.0">
         <children>
            <PasswordField fx:id="keyField" onKeyTyped="#passwordChanged" />
            <Button mnemonicParsing="false" onAction="#setKey" text="Set Key" />
         </children>
      </HBox>
   </children>
</AnchorPane>
